<Window x:Class="IntelliTraceCPConfig.AddModulePattern"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Add a Pattern" Height="162" Width="230" WindowStartupLocation="CenterOwner">
    <Grid>
        <Label Content="Enter a string pattern to match:" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="12,38,0,0" Name="txtPattern" VerticalAlignment="Top" Width="175" />
        <Button Content="Add" Height="23" HorizontalAlignment="Left" Margin="12,80,0,0" Name="btnAdd" VerticalAlignment="Top" Width="75" Click="BtnAddClick" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Right" Margin="0,80,21,0" Name="btnCancel" VerticalAlignment="Top" Width="75" />
    </Grid>
</Window>
